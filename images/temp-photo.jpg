.title-wrap{display:flex;flex-direction:row;align-items:center;}
.title-wrap + *{margin-top:30px;}
.title-wrap .right{margin-left:auto;}
.title-wrap .right .search-menu > * ~ *{margin-left:5px;}
.title-wrap .vt{align-items: flex-start;}
.title-wrap .btn-more{ color:#777;font-size:14px;}
.title-wrap.title-search{align-items: flex-start;}
[class*=title-] em{vertical-align:baseline; color:#3441D5;}
[class*=title-primary] { font-size: 26px; font-weight: 500; color: #101828;}
[class*=title-primary] .sub-txt{ color:#666;font-size:20px;}
.title-major{display:block;font-size:18px;font-weight:500;}
.title-junior{display:block;font-size:14px;margin-bottom:6px;font-weight:400;margin-top:20px;}

/* .title-wrap * ~ .relative-word{padding-left:15px;margin-left:15px;position:relative;}
.title-wrap * ~ .relative-word:before{content:'';display:block;width:1px;height:15px;background:#BBB;position:absolute;top:50%;left:0;transform:translateY(-50%);-webkit-transform:translateY(-50%);} */
.title-wrap * ~ .relative-word{margin-top:10px;}
.relative-word{display:flex;flex-direction:row;align-items:center;}
.relative-word > b{color: #555; font-size: 14px; font-weight: 500; line-height: 10px;white-space: nowrap;margin-right:15px;}
.relative-word .words{display:flex;flex-direction:row;align-items:center;gap: 5px;}
.relative-word .words > span{display:inline-flex;border-radius: 4px;align-items: center;padding:0 10px;	background: #ECE7E7;height:22px; color:#444;font-size:12px;}

.layer-wrap{position:relative;display:inline-block;}
.layer-wrap .btn-more{width:30px;height:40px;background:url('../img/ico-more.svg') no-repeat 50% 50%;}
.layer-wrap .menu-layer{display:none;position:absolute;top:calc(100% + 8px);max-height: 50vh;overflow:hidden;overflow-y:auto;border-radius: 8px; border: 1px solid #f0f1f1; background: #fff; box-shadow: 0px 4px 6px -2px rgba(16, 24, 40, 0.03), 0px 12px 16px -4px rgba(16, 24, 40, 0.06);z-index: 10;}
.layer-wrap .menu-layer::-webkit-scrollbar{-webkit-appearance:none;}
.layer-wrap .menu-layer::-webkit-scrollbar:vertical{width:5px;}
.layer-wrap .menu-layer::-webkit-scrollbar:horizontal{height:5px;} 
.layer-wrap .menu-layer::-webkit-scrollbar-thumb{ background-color:rgba(0,0,0,.2); border-radius:10px; }
.layer-wrap .menu-layer::-webkit-scrollbar-track{ background-color:rgba(0,0,0,.05); border-radius:10px; }
.layer-wrap .menu-layer.right{right:0;left:initial;}
.layer-wrap .menu-layer ul li > *{display:flex;align-items:center;height:40px;font-size:14px;padding:0 16px;white-space: nowrap;}
.layer-wrap .menu-layer ul li > * i:first-child{margin-right:12px;}
.layer-wrap .menu-layer ul li > *:hover{ color:#3441D5;}
.layer-wrap .menu-layer ul li ~ li{border-top:1px solid #EAECF0;}

.tag-type{display:inline-flex;height:24px;align-items: center;padding:0 10px;border-radius:24px;}
.tag-type.st1{color:#6941C6;border:1px solid #6941C6;}

/* tooltip */
.tooltip-wrap{position:relative;display:inline-flex;white-space: nowrap;align-items: center;cursor: pointer;z-index: 6;}
.tooltip-wrap > * ~ i{margin-left:10px;}
.tooltip-wrap:hover .tooltip-layer{display:block;}
.tooltip-wrap .tooltip-layer{display:none;position:absolute;top:calc(100% + 10px);left:50%;transform:translate(-50%, 0);background:#fff; color:#777;font-size:11px;padding:12px;border-radius:8px;border:1px solid #aaa;font-family: 'NotoSans';}
.tooltip-wrap .tooltip-layer .pos{position:relative;}
.tooltip-wrap .tooltip-layer:after{content:'';display:block;position:absolute;top:-8.5px;left:50%;width :0;height:0;border-left:8px solid transparent;border-right:8px solid transparent;border-bottom:10px solid #fff;transform:translateX(-50%);-webkit-transform:translateX(-50%);}
.tooltip-wrap .tooltip-layer:before{content:'';display:block;position:absolute;top:-10px;left:50%;width :0;height:0;border-left:8px solid transparent;border-right:8px solid transparent;border-bottom:10px solid #aaa;transform:translateX(-50%);-webkit-transform:translateX(-50%);}
.tooltip-wrap .tooltip-layer .tooltip-txt,
.tooltip-wrap .tooltip-layer p{font-size:11px; color:#777;font-weight:500;display:block;}
.tooltip-wrap .tooltip-layer .txt-star:before{content:'*';display:inline;}
.tooltip-wrap .tooltip-layer .txt-dot{display:block;position:relative;padding-left:10px;}
.tooltip-wrap .tooltip-layer .txt-dot:before{content:'';display:block;position:absolute;top:5px;left:0;width:4px;height:4px;background:#aaa;border-radius:100%;}
.tooltip-wrap .tooltip-layer .txt-dot ~ .txt-dot{margin-top:4px;}
.tooltip-wrap .tooltip-layer .tit{display:block;font-size:11px;font-weight:700; color:#383838;margin-bottom:8px;}
.tooltip-wrap.type2 .ico-noti{position:relative;z-index: 1;}
.tooltip-wrap.type2 .ico-noti:before,
.tooltip-wrap.type2 .ico-noti:after{content:'';display:none;position:absolute;top:22.5px;left:50%;width :0;height:0;border-left:8px solid transparent;border-right:8px solid transparent;border-bottom:10px solid #ffffff;transform:translateX(-50%);-webkit-transform:translateX(-50%);}
.tooltip-wrap.type2 .ico-noti:before{top:21px;border-bottom-color:#aaa;}
.tooltip-wrap.type2 .tooltip-layer:before,
.tooltip-wrap.type2 .tooltip-layer:after{display:none;}
.tooltip-wrap.type2:hover .ico-noti:before,
.tooltip-wrap.type2:hover .ico-noti:after{display:block;}
.file-add-tooltip .tooltip-layer{left:100px;left:-50px;transform:translateX(0);}
.tooltip-wrap.floating .tooltip-layer {position:fixed;top:initial;left:initial;transform: initial;max-width: 300px;}
.tooltip-wrap.floating .tooltip-layer .tooltip-txt {white-space: normal;}
.tooltip-wrap.floating:hover{z-index: 10;}

/* btn */
.data-type4 ~ .btn-wrap,
.paging-wrap ~ .btn-wrap{margin-top:15px;}
.btn-wrap{display:flex;align-items:center;justify-content: center;margin-top:30px;}
.btn-wrap [class*=btn-type] ~ [class*=btn-type]{margin-left:13px;}
.btn-wrap .left{margin-right:auto;}
.btn-wrap .right{margin-left:auto;display:flex;align-items:center;}
.btn-wrap.line-st{border-top:1px solid #D0D5DD;padding-top:20px;}
.section + .btn-wrap.line-st{margin-top:0 !important;}
[class*=btn-type] {display:inline-flex;text-align:center;align-items: center;justify-content: center;white-space: nowrap;}
[class*=btn-type]:disabled {background-color: #DDD !important;color: #aaaaac !important;border-color: #DDD !important;}
[class*=btn-type].st1{border:1px solid #151A59;color: #fff;background:#151A59;box-shadow: 0px 1px 2px 0px rgba(16, 24, 40, 0.05);}
[class*=btn-type].st2{border:1px solid #D0D5DD;color: #344054;background:#fff;box-shadow: 0px 1px 2px 0px rgba(16, 24, 40, 0.05);}
[class*=btn-type].st3{border:1px solid #D92D20;color: #fff;background:#D92D20;box-shadow: 0px 1px 2px 0px rgba(16, 24, 40, 0.05);}
[class*=btn-type].st4{border:1px solid #151A59;color: #151A59;background:#fff;box-shadow: 0px 1px 2px 0px rgba(16, 24, 40, 0.05);}
[class*=btn-type].st5{border:1px solid #D0D5DD;color: #344054;background:#F9FAFB;}
[class*=btn-type].st6{border:1px solid #EEEFFC;color: #3441D5;background:#EEEFFC;box-shadow: 0px 1px 2px 0px rgba(16, 24, 40, 0.05);}
[class*=btn-type].st7{border:1px solid #D0D5DD;color: #344054;background:#D0D5DD;}
[class*=btn-type].st8{box-shadow: 0px 1px 2px 0px rgba(16, 24, 40, 0.05); color:#667085;border:1px solid #D0D5DD;background:#fff;}
[class*=btn-type].st9{border: 1px solid #6771E0; background: #6771E0; color:#ffff;}
[class*=btn-type].st10{border: 1px solid #27C166; background: #fff; color:#27C166;}
[class*=btn-type].st11{border: 1px solid #27C166; background: #27C166; color:#fff;}
[class*=btn-type].st12{border: 1px solid #6771E0; background: #6771E0; color:#fff;}
[class*=btn-type].st13{border: 1px solid #4F5EFA; background: #4F5EFA; color:#fff;}
[class*=btn-type].full{width:100%;}
[class*=btn-type] span{line-height:1;}
[class*=btn-type] i:first-child{margin-right:6px;}
[class*=btn-type] i:only-child{margin:0;}
.btn-type1{height:44px;line-height:42px;padding:0 40px;text-align:center;font-size:16px;border-radius: 8px;}
.btn-type1.icon,
.btn-type2.icon{width:44px;padding:0 0;}
.btn-type2{height:44px;line-height:40px;padding:0 20px;text-align:center;font-size:16px;border-radius: 8px;}
.btn-type3{height:36px;line-height:34px;padding:0 14px;text-align:center;font-size:14px;border-radius: 8px;}
.btn-type4{height:48px;line-height:46px;padding:0 14px;text-align:center;font-size:16px;border-radius: 8px;}
.btn-type5{height:24px;line-height:22px;padding:0 12px;text-align:center;font-size:12px;border-radius: 4px;}

.ellips{overflow:hidden;text-overflow:ellipsis;display:block;-webkit-line-clamp:1;-webkit-box-orient:vertical;word-wrap:break-word;line-height:1.2em;height:1.3em;}
.pre-line{white-space: pre-line;}

/* loadgin */
.loading {position:fixed;top:0;left:0;width:100%;display:flex;justify-content:center;align-items:center;height:100%;background:rgba(0,0,0,0.4);}
.spinner { height:40px; width:40px; position: absolute; top:50%; left:50%; margin: -20px 0 0 -20px; border:2px solid #eee;border-radius:100%; -webkit-animation: rotation 1s infinite linear; animation: rotation 1s infinite linear;}
.spinner:before {content:""; display:block; position:absolute; left:-2px; top:-2px; height:40px; width:40px; border-radius:100%; border-top:2px solid #1f5dbe; border-left:2px solid rgba(0,0,0,0.1); border-bottom:2px solid rgba(0,0,0,0.1); border-right:2px solid rgba(0,0,0,0.1);}
@keyframes rotation { from {transform: rotate(0deg);} to {transform: rotate(359deg);} }
